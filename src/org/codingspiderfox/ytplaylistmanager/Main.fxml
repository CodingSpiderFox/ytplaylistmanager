<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.codingspiderfox.ytplaylistmanager.MainController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0">
         <children>
            <VBox prefHeight="600.0" prefWidth="800.0">
               <children>
                  <MenuBar nodeOrientation="LEFT_TO_RIGHT">
                    <menus>
                      <Menu mnemonicParsing="false" text="File">
                        <items>
                          <MenuItem mnemonicParsing="false" text="Close" />
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Edit">
                        <items>
                          <MenuItem mnemonicParsing="false" text="Delete" />
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Help">
                        <items>
                          <MenuItem mnemonicParsing="false" text="About" />
                        </items>
                      </Menu>
                    </menus>
                  </MenuBar>
                  <HBox prefHeight="500.0" prefWidth="800.0">
                     <children>
                        <VBox prefHeight="200.0" prefWidth="250.0">
                           <children>
                              <AnchorPane prefHeight="50.0" prefWidth="250.0">
                                 <children>
                                    <Label prefHeight="25.0" prefWidth="250.0" text="Label" />
                                 </children>
                              </AnchorPane>
                              <ListView prefHeight="500.0" prefWidth="250.0" />
                           </children>
                        </VBox>
                        <VBox layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="250.0">
                           <children>
                              <Pane prefHeight="50.0" prefWidth="250.0" />
                              <ListView prefHeight="500.0" prefWidth="250.0" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</GridPane>
